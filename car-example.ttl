@base <http://data-webapp.hugonlabs.com/test1/> .
@prefix sdtw: <http://ontology.hugonlabs.com/sdtw#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix quantityKind: <http://qudt.org/vocab/quantitykind/> .

<users/jhugon>
    a sosa:Sensor ;
    a foaf:Person ;
    foaf:name "Justin Hugon" ;
    ssn:detects <stimuli/car1/2022-07-10T00:00:00Z> ;
    .

<features/car1>
    a sosa:FeatureOfInterest ;
    rdfs:label "Car1" ;
    rdfs:comment "Example car" ;
    .

<properties/car1/odometer>
    a sosa:ObservableProperty ;
    rdfs:label "Car1's odometer" ;
    rdfs:comment "Car1's odometer reading" ;
    ssn:isPropertyOf <features/car1>;
    sdtw:hasQuantityKind quantityKind:Length ;
    sdtw:hasUnit unit:MI ;
    .

<properties/car1/gasqty>
    a sosa:ObservableProperty ;
    rdfs:label "Car1 gas quantity" ;
    rdfs:comment "The amount of gasoline pumped into Car1's gas tank" ;
    ssn:isPropertyOf <features/car1>;
    sdtw:hasQuantityKind quantityKind:LiquidVolume ;
    sdtw:hasUnit unit:GAL_US ;
    .

<stimuli/car1/2022-07-10T00:00:00Z>
    a ssn:Stimulus ;
    rdfs:label "Car1 stimulus at 2022-07-10T00:00:00Z" ;
    rdfs:comment "Decided to stop for gas" ;
    sdtw:hasTime "2022-07-10T00:00:00Z"^^xsd:dateTime ;
    ssn:isProxyFor <properties/car1/odometer> , <properties/car1/gasqty> ;
    .

<observations/car1/odometer/2022-07-10T00:00:00Z>
    a sosa:Observation ;
    rdfs:label "Car1 odometer obs at 2022-07-10T00:00:00Z" ;
    sosa:madeBySensor <users/jhugon> ;
    ssn:wasOriginatedBy <stimuli/car1/2022-07-10T00:00:00Z> ;
    sosa:hasFeatureOfInterest <features/car1> ;
    sosa:observedProperty <properties/car1/odometer> ;
    sosa:resultTime "2022-07-10T00:00:00Z"^^xsd:dateTime ;
    sosa:hasResult [
        rdf:type sosa:Result ;
        rdf:type qudt:Quantity ;
        qudt:unit unit:MI ;
        qudt:value "12345"^^xsd:float ] ;
    .

<observations/car1/gasqty/2022-07-10T00:00:00Z>
    a sosa:Observation ;
    rdfs:label "Car1 gas quantity obs at 2022-07-10T00:00:00Z" ;
    sosa:madeBySensor <users/jhugon> ;
    ssn:wasOriginatedBy <stimuli/car1/2022-07-10T00:00:00Z> ;
    sosa:hasFeatureOfInterest <features/car1> ;
    sosa:observedProperty <properties/car1/gasqty> ;
    sosa:resultTime "2022-07-10T00:00:00Z"^^xsd:dateTime ;
    sosa:hasResult [
        rdf:type sosa:Result ;
        rdf:type qudt:Quantity ;
        qudt:unit unit:GAL_US ;
        qudt:value "11.2"^^xsd:float ] ;
    .

<stimuli/car1/2022-07-11T00:00:00Z>
    a ssn:Stimulus ;
    rdfs:label "Car1 stimulus at 2022-07-11T00:00:00Z" ;
    rdfs:comment "Decided to stop for gas" ;
    sdtw:hasTime "2022-07-11T00:00:00Z"^^xsd:dateTime ;
    ssn:isProxyFor <properties/car1/odometer> ;
    .

<observations/car1/odometer/2022-07-11T00:00:00Z>
    a sosa:Observation ;
    rdfs:label "Car1 odometer obs at 2022-07-11T00:00:00Z" ;
    sosa:madeBySensor <users/jhugon> ;
    ssn:wasOriginatedBy <stimuli/car1/2022-07-11T00:00:00Z> ;
    sosa:hasFeatureOfInterest <features/car1> ;
    sosa:observedProperty <properties/car1/odometer> ;
    sosa:resultTime "2022-07-11T00:00:00Z"^^xsd:dateTime ;
    sosa:hasResult [
        rdf:type sosa:Result ;
        rdf:type qudt:Quantity ;
        qudt:unit unit:MI ;
        qudt:value "12500"^^xsd:float ] ;
    .
