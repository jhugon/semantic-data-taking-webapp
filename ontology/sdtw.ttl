@prefix : <http://ontology.hugonlabs.com/sdtw#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qudt: <http://qudt.org/2.1/schema/qudt> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdtw: <http://ontology.hugonlabs.com/sdtw#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://ontology.hugonlabs.com/sdtw#> .

<http://ontology.hugonlabs.com/sdtw#> rdf:type owl:Ontology ;
                                       owl:imports <http://qudt.org/2.1/schema/qudt> ,
                                                   sosa: ,
                                                   ssn: ;
                                       dcterms:created "2021-07-08"^^xsd:date ;
                                       dcterms:creator "Justin Hugon"@en ;
                                       dcterms:description "This ontology aids the Semantic Data Taking Web-app."@en ;
                                       dcterms:license <https://www.gnu.org/licenses/gpl-3.0.en.html> ;
                                       dcterms:rights "Copyright 2021 Justin Hugon." ;
                                       dcterms:title "Semantic Data Taking Web-app Ontology"@en ;
                                       rdfs:seeAlso <https://github.com/jhugon/semantic-data-taking-webapp> .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/created
dcterms:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/rights
dcterms:rights rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://ontology.hugonlabs.com/sdtw#hasObservableType
sdtw:hasObservableType rdf:type owl:ObjectProperty ;
                       rdfs:domain sosa:ObservableProperty ;
                       rdfs:range sdtw:ObservableType .


###  http://ontology.hugonlabs.com/sdtw#hasQualityKind
sdtw:hasQualityKind rdf:type owl:ObjectProperty ;
                    rdfs:domain sosa:ObservableProperty ;
                    rdfs:range qudt:QuantityKind .


###  http://ontology.hugonlabs.com/sdtw#hasTime
sdtw:hasTime rdf:type owl:ObjectProperty ;
             rdfs:domain ssn:Stimulus ;
             rdfs:range xsd:dateTime .


###  http://ontology.hugonlabs.com/sdtw#hasUnit
sdtw:hasUnit rdf:type owl:ObjectProperty ;
             rdfs:domain sosa:ObservableProperty ;
             rdfs:range qudt:Unit .


#################################################################
#    Classes
#################################################################

###  http://ontology.hugonlabs.com/sdtw#ObservableType
sdtw:ObservableType rdf:type owl:Class .


###  http://qudt.org/2.1/schema/qudtQuantityKind
qudt:QuantityKind rdf:type owl:Class .


###  http://qudt.org/2.1/schema/qudtUnit
qudt:Unit rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://ontology.hugonlabs.com/sdtw#categorical
sdtw:categorical rdf:type owl:NamedIndividual ,
                          sdtw:ObservableType .


###  http://ontology.hugonlabs.com/sdtw#quantitative
sdtw:quantitative rdf:type owl:NamedIndividual ,
                           sdtw:ObservableType .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( sdtw:categorical
                        sdtw:quantitative
                      )
] .


